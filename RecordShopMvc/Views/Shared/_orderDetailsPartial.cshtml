@using RecordShopClassLibrary.Models.Read
@model IEnumerable<OrderDetailProductViewModel>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Product</th>
      <th scope="col">Price</th>
      <th scope="col">Category</th>
      <th scope="col">Quantity</th>
    </tr>
  </thead>
 <tbody>
            @if(Model != null)
            {
             foreach(var od in Model)
            {

                <partial name="_orderProductPartial" title="" model="od"/>
            }
            }

 </tbody>
</table>